{
  "expo": {
    "name": "Film Fare",
    "slug": "filmfare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo1.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121225"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.sanskaar.filmfare",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo1.png",
        "backgroundColor": "#121225"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
